int T0 = second();
int T1 = second();
int At = 10 ;

void main(){
  animate(temporizador);
}

void temporizador(){
	background (0,0,0);
	T1 = second() ;
  if(T0+At-T1 == 0){
    T0 = second();
  }
  
 
  
  textSize(20);
  text("cuenta atras: " + (T0+At-T1) ,60,120);
  text("TO: " + T0 ,160,160);
  text("T1: " +T1 ,160,200);
 
  
}

