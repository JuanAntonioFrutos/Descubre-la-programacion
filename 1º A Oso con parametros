

void oso(int x, int y, int escala){
  // orejas
  fill(255,255,255);
  ellipse(x-80,y-80,100,100);
  ellipse(x+80,y-80,100,100);
// cara
  fill(255,254,254);
  ellipse (x,y,200,200);

//ojos
  noFill();
  ellipse(x-40,y-40,50,50);
  ellipse(x+40,y-40,50,50);
  fill(0,0,0);
  ellipse(x-40,y-40,5,5);
  ellipse(x+40,y-40,5,5);

// nariz
  noFill();
  ellipse(x,y,30,10);

// boca
  ellipse(x,y+40,80,10);


}


void main() {
   oso(160,160,1);
 
}
